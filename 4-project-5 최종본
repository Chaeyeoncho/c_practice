#include <stdio.h>

int main(void) {

    int a, b, c, d, e, f, g, h, i, j, k, sum1, sum2, finalsum;

    printf("Enter the first 11 digits of a UPC:  ");
    scanf_s("%1d%1d%1d%1d%1d%1d%1d%1d%1d%1d%1d", &a, &b, &c, &d, &e, &f, &g, &h, &i, &j, &k);
    
    sum1 = a + c + e + g + i + k;
    sum2 = b + d + f + h + j;
    finalsum = sum1 * 3 + sum2 - 1;
    i = 9-(finalsum % 10);

    printf("Check digit: %d", i);

    return 0;
}
