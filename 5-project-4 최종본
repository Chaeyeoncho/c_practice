#include <stdio.h>

int main(void) {
    
    int speed;
    printf("enter a wind speed (in knots):  ");
    scanf_s("%d", &speed);
    if (speed < 1)
    {
        printf("calm");
    }else if (1 <= speed && speed <= 3)
    {
        printf("Light air");
    }
    else if (4 <= speed && speed <= 27)
    { 
        printf("Breeze");
    }
    else if (28 <= speed && speed <= 47)
    { 
        printf("Gale");
    }
    else if (48 <= speed && speed <= 63)
    {
        printf("Storm"); 
    }
    else if (speed >=63)
    { 
        printf("Hurricane");
    }
    return 0;
}
